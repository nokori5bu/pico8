pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
hz=87
end


 function limit()
 if hz<87 then
 hz=87 end
 if hz>108 then
 hz=108 end
 end

 function countral()
 if btn(2) then hz+=0.5 end
 if btn(3) then hz-=0.5 end
 end

 function _update()
 countral()
 limit()
 end
 
 function redline()
 rect(63,0,63,60,8)
 rect(63,70,63,127,8)
 print(hz,60,63,8)
 end
 
 function whiteline()
  for i=1,50 do
  rect(i*50-hz*5,0,i*50-hz*5,30,7)
  rect(i*50-hz*5,97,i*50-hz*5,127,7)
  end
  for i=1,250 do
  rect(i*10-hz*5,0,i*10-hz*5,15,7)
  rect(i*10-hz*5,112,i*10-hz*5,127,7)
  end
 end
 
function _draw()
cls(0)
redline()
whiteline()
end
